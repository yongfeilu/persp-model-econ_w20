    "import pandas as pd\n",
    "W_hat = np.eye(4)\n",
    "params_init = np.array([11.5, 0.9 ])\n",
    "gmm_args = (hh_inc_synth, W_hat)\n",
    "res = opt.minimize(criterion4, params_init, args=(gmm_args),\n",
    "                   tol=1e-13, method='L-BFGS-B',\n",
    "                   bounds=((1e-10, None),(1e-10, None)))\n",
    "mu_GMM, sig_GMM = res.x\n",
